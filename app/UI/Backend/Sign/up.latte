{* subtitle sites *}
{block title}{_'New user registration'}{/block}

{* website content *}
{block content}
{embed ../../../Core/Widget/@form.latte, name: 'signUp'}
	{block form}
		{include input, name: 'username'}
		{include input, name: 'email'}
		{include input, name: 'password'}
		{include input, name: 'verify'}
		<div class="d-grid gap-1">
			{include submit, name: 'send'}
			<a n:href="in" class="btn btn-link text-start">{_'Back'}</a>
		</div>
	{/block}
{/embed}
