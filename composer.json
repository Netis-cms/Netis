{
	"name": "netis-cms/netis",
	"type": "project",
	"description": "Basis for new projects on Drago Extension",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Zdeněk Papučík",
			"homepage": "https://github.com/accgit"
		}
	],
	"require": {
		"php": ">=8.1",
		"nette/robot-loader": "^3.4",
		"nette/security": "^3.1",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.9",
		"drago-ex/bootstrap": "^1.0",
		"drago-ex/translator": "^1.0",
		"drago-ex/application": "dev-master",
		"drago-ex/parameters": "dev-master",
		"drago-ex/database": "dev-master",
		"drago-ex/utils": "^1.0",
		"drago-ex/authorization": "dev-master",
	  	"texy/texy": "^3.1"
	},
	"require-dev": {
		"phpstan/phpstan-nette": "^1.2.9"
	},
	"autoload": {
		"psr-4": {"App\\": "app"}
	},
	"scripts": {
		"phpstan": "phpstan analyse --level 5 --configuration phpstan.neon app"
	},
	"minimum-stability": "dev",
	"prefer-stable" : true
}
